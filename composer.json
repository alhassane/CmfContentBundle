{
  "name": "sfynx-project/cmf-plugin-content-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provide a plugin with several widgets like Article, Tag, media, rub, test to work with the CMF",
  "keywords": ["cmf", "widget"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/CmfContentBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Cmf\\ContentBundle": "" }
  },

  "target-dir": "Cmf/ContentBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.8-dev"
    }
  }
}
